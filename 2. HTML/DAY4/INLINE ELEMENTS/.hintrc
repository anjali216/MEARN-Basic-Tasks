{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/aria": [
      "default",
      {
        "empty-table-header": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]",
          "iframe[loading]"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}